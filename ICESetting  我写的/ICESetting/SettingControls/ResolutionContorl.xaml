<UserControl x:Class="ICESetting.SettingControls.ResolutionContorl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ice="clr-namespace:ICESetting.Control"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Width="800"
             d:DesignHeight="100"
             mc:Ignorable="d">
    <Grid Margin="0,0,0,10">
        <StackPanel Margin="0,37,0,0" Orientation="Horizontal">
            <TextBlock HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       FontFamily="微软雅黑"
                       FontSize="30"
                       Foreground="#ffffff"
                       Text="分辨率设置" />
            <Grid x:Name="resolutionUIGrid"
                  Width="530"
                  Height="52"
                  Margin="45,0,0,0">
                <ice:ResolutionUI x:Name="resolutionUI"
                                  HorizontalAlignment="Center"
                                  VerticalAlignment="Bottom" />
            </Grid>
        </StackPanel>
    </Grid>
</UserControl>
